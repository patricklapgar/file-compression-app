{
  "name": "image-compress",
  "productName":"ImageCompress",
  "version": "1.0.0",
  "description": "App to compress image files without reducinng overall image quality",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ImageCompress\"",    
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "author": "Patrick Apgar",
  "license": "MIT",
  "devDependencies": {
    "electron": "^19.0.4",
    "electron-packager": "^15.5.1",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "slash": "^3.0.0"
  }
}
